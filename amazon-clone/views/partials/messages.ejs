<% if(typeof errors!='undefined'){ %>
    <%errors.forEach(function(error){%>
        <div class="1 alert alert-warning alert-dismissible fade show" role="alert">
            <%=error.msg%>
            <button type="button" class="close" data-dismiss="alert" onclick="remove()" aria-label="Close">
              <span aria-hidden="true"> &times; </span>
            </button>
          </div>
    <%})%>
<% } %>

<% if(success_msg!=''){ %>
  <div class="1 alert alert-warning alert-dismissible fade show" role="alert">
    <%=success_msg%>
    <button type="button" class="close" data-dismiss="alert" onclick="remove()" aria-label="Close">
      <span aria-hidden="true"> &times; </span>
    </button>
  </div>
<%}%>

<% if(error_msg!=''){ %>
  <div class="1 alert alert-warning alert-dismissible fade show" role="alert">
    <%=error_msg%>
    <button type="button" class="close" data-dismiss="alert" onclick="remove()" aria-label="Close">
      <span aria-hidden="true"> &times; </span>
    </button>
  </div>
<%}%>
<% if(error!=''){ %>
  <div class="1 alert alert-warning alert-dismissible fade show" role="alert">
    <%=error%>
    <button type="button" class="close" data-dismiss="alert" onclick="remove()" aria-label="Close">
      <span aria-hidden="true"> &times; </span>
    </button>
  </div>
<%}%>